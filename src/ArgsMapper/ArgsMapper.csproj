<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Title>Command-Line Arguments Mapper</Title>

    <AssemblyVersion>0.1.0</AssemblyVersion>
    <FileVersion>0.1.0</FileVersion>
    <VersionPrefix>0.1.0</VersionPrefix>
    <VersionSuffix>local</VersionSuffix>

    <Authors>afriron</Authors>
    <Product>ArgMapper</Product>
    <Copyright>Copyright Serbaev Vladimir 2020. All rights reserved.</Copyright>
    <Description>This library is designed for mapping command line arguments to an object.</Description>
    
    <PackageId>ArgMapper</PackageId>
    <PackageTags>Console;Arguments;Parser;Mapper</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/afriron/argsmapper</RepositoryUrl>
  </PropertyGroup>
</Project>
